{
    "name": "andrisarkameru/otphp",
    "type": "library",
    "description": "PHP OTP Library compatible with Google Authenticator",
    "license": "MIT",
    "keywords": ["otp", "hotp", "totp"],
    "homepage": "https://github.com/andrisarkameru/otphp",
    "authors": [
        {
            "name": "Spomky-Labs",
            "homepage": "https://github.com/Spomky-Labs/otphp/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "christian-riesen/base32": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "OTPHP\\": "lib/" },
        "psr-0": {"OTPHP": "lib/"}
    },
    "autoload-dev": {
        "psr-4": {
            "MyProject\\": "examples/"
        },
        "psr-0": {"OTPHP": "lib/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
